<Window x:Class="Diagnostick.DiagnosticsOrganizationsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Diagnostick"
        mc:Ignorable="d"
        Title="Участники диагностики (организации)" Height="400" Width="650">
    <Grid>
        <StackPanel>
            <StackPanel Orientation="Horizontal">
                <DatePicker x:Name="FromDate" Width="150" Margin="5"/>
                <DatePicker x:Name="ToDate" Width="150" Margin="5"/>
                <Button Content="Поиск" Click="SearchButton_Click" Width="100" Margin="5"/>
            </StackPanel>
            <DataGrid x:Name="OrganizationGrid" AutoGenerateColumns="True" Margin="5"/>
        </StackPanel>
    </Grid>
</Window>
